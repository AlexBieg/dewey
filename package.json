{
  "name": "dewey",
  "version": "0.1.0",
  "description": "An NPM package for testing directory structures",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "dewey": "dist/index.js"
  },
  "watch": {
    "build": "{index,dewey}.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "postinstall": "npm run build",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexBieg/dewey.git"
  },
  "author": "Alex Bieg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexBieg/dewey/issues"
  },
  "homepage": "https://github.com/AlexBieg/dewey#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "npm-watch": "^0.6.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "chalk": "^2.4.2"
  }
}
